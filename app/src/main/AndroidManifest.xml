<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hebeu.meet">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/orange"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/orange"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".HomeActivity"
            android:windowSoftInputMode="adjustPan">
        </activity>
        <activity
            android:name=".My_Information"
            android:label="@string/my_information">

        </activity>
        <activity
            android:name=".Update_My_Information"
            android:label="@string/update_my_information"
            android:parentActivityName=".My_Information">

        </activity>
        <activity
            android:name=".Register"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/myNotitle">
        </activity>
        <activity
            android:name=".My_Publish_Activity"
            android:label="@string/my_publish_activity">
        </activity>
        <activity android:name=".Others_Apply_Activity"></activity>
        <activity android:name=".MyApplyActivity"
            android:label="@string/my_apply_activity">

        </activity>
        <activity android:name=".Login"
            android:theme="@style/myNotitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Details"
            android:label="@string/details">

        </activity>
        <activity android:name=".ApplyJoin"></activity>

        <activity
            android:name=".EmptyActivity"
            android:label="@string/my_publish_activity"></activity>

        <activity android:name=".ContactList"></activity>
        <activity
            android:name=".Forget_Password"
            android:label="@string/forgrtpwd"
            android:parentActivityName=".Login">
        </activity>
    </application>

</manifest>